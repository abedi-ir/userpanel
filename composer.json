{
    "name": "jalno/userpanel",
    "description": "Userpanel for laravel apps.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/validation": "^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/pagination": "^8.0",
        "illuminate/http": "^8.0",
        "laravel/passport": ">=0.2.2",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jalno\\Userpanel\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Jalno\\Userpanel\\Providers\\UserpanelServiceProvider",
                "Jalno\\Userpanel\\Providers\\AuthorizationServiceProvider"
            ]
        }
    }
}
